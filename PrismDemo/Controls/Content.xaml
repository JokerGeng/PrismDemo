<UserControl x:Class="PrismDemo.Content"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PrismDemo"
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             mc:Ignorable="d"
             d:DesignHeight="500" d:DesignWidth="500">
    <Border Background="#948CC0">
        <StackPanel>
            <TextBlock Text="{Binding Content}" FontSize="30"/>
            <Button Content="Pub" FontSize="30" Command="{Binding Pub}"/>
            <Button Content="PubStr" FontSize="30" Command="{Binding PubStr}" CommandParameter="PubCommandStrParameter"/>
        </StackPanel>
    </Border>
</UserControl>
